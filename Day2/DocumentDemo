f = open("D:\\next.txt")
print(f.read())
f.close()
f = open("D:\\next.txt")
print(f.read(2))
f.close()
f = open("D:\\next.txt")
print(f.tell())
f.close()
f = open("D:\\next.txt")
print(f.seek(2, 0))
f.close()
f = open("D:\\next.txt")
print(list(f.read()))
f.close()
f = open("D:\\next.txt")
print(list(f.readline()))
f.close()
f = open("D:\\next.txt")
f.seek(0, 0)
